@using VendorTracker.Models
<h1>Vendor Details: </h1>

<ul>
  @foreach (Order order in Model["orders"])
  {
    <li>Order Name: @order.Title</li>
    <li>Date Ordered: @order.Date</li>
    <li>Price: @order.Price</li>
    <li>Order Description: @order.Description</li>
  }
</ul>
<a href='/vendors'>Return to All Vendors </a>
<a href='/vendors/@Model["vendor"].Id/orders/new'>Add an order for this vendor.</a>